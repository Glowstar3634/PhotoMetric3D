{
  "expo": {
    "name": "PhotoMetric3D",
    "slug": "PhotoMetric3D",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./constants/images/PM3DLogo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./constants/images/PM3DLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c1c18"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./constants/images/PM3DLogo.png",
        "backgroundColor": "#1c1c18"
      },
      "package": "com.senn.PhotoMetric3D"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins":["react-native-ble-plx"]
  }
}
